{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "React/Webpack boilerplate",
  "main": "server.js",
  "scripts": {
    "dev": "webpack-dev-server --host 0.0.0.0 --config webpack.config.js --watch --colors",
    "build": "rm -rf dist/ && NODE_ENV=production webpack --colors --profile",
    "start": "PORT=8080 pm2 start ./server.js",
    "test": "npm test"
  },
