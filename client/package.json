    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "exports-loader": "^0.6.3",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.6.5",
    "material-ui": "^0.16.1",
    "node-sass": "^3.11.2",
    "pm2": "^2.1.4",
    "raphael": "^2.2.6",
    "raw-loader": "^0.5.1",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
